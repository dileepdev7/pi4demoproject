# SQL query:,"SELECT ""traits_and_yields_view"".* FROM ""traits_and_yields_view""  WHERE (traits_and_yields_view.access_level >= 4) AND ((LOWER(sitename) LIKE LOWER('%saliz%') OR LOWER(city) LIKE LOWER('%saliz%') OR LOWER(scientificname) LIKE LOWER('%saliz%') OR LOWER(commonname) LIKE LOWER('%saliz%') OR LOWER(author) LIKE LOWER('%saliz%') OR LOWER(trait) LIKE LOWER('%saliz%') OR LOWER(trait_description) LIKE LOWER('%saliz%') OR LOWER(cultivar) LIKE LOWER('%saliz%') OR LOWER(entity) LIKE LOWER('%saliz%') OR citation_year::text = 'saliz')) AND (checked >= 1)"
